import java.util.Scanner;


public class Betyg {
	public static void main(String[] args) {
		
		int IG=0;
		int medel=0;
		int antalbetyg=0;
		int betyg=0;
		
		System.out.println("Skriv ett antal betyg (0, 3, 4 eller 5)");
		Scanner scan = new Scanner(System.in);
		while(betyg>=0) {
			
		antalbetyg++;
		betyg = scan.nextInt();
		
		if(betyg==0) {
			IG++;
		}
		
		else {
			medel=medel+betyg;
		}
			
		}
		
		System.out.println("Antal inl채sta betyg:" + antalbetyg);
		System.out.println("Antalet underk채nda betyg:" + IG);
		System.out.println("Medelv채rde av de godk채nda betygen:" + medel/antalbetyg);

		
	}

}
