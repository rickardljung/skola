import java.util.Scanner;


public class Betyg {
	public static void main(String[] args) {
		
		int IG=0;
		int medel=0;
		int antalIGbetyg=0;
		int antalGbetyg=0;
		int betyg=0;
		
		System.out.println("Skriv ett antal betyg (0, 3, 4 eller 5)");
		Scanner scan = new Scanner(System.in);
		while(betyg>=0) {
			
		
		betyg = scan.nextInt();
		
		if(betyg==0) {
			IG++;
			antalIGbetyg++;
		}
		
		if(betyg>0) {
			medel=medel+betyg;
			antalGbetyg++;
		}
			
		}
		
		System.out.println("Antal inl채sta betyg:" + (antalGbetyg+antalIGbetyg));
		System.out.println("Antalet underk채nda betyg:" + IG);
		System.out.println("Medelv채rde av de godk채nda betygen:" + medel/antalGbetyg);

		
	}

}
